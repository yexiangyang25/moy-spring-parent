<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.moy.spring.test.example.repository.PasswordRepository">
    <resultMap id="BaseResultMap" type="org.moy.spring.test.example.domain.PasswordEntity">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="code" property="code" jdbcType="VARCHAR"/>
        <result column="user_code" property="userCode" jdbcType="VARCHAR"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="modify_time" property="modifyTime" jdbcType="TIMESTAMP"/>
        <result column="operator" property="operator" jdbcType="VARCHAR"/>
        <result column="operate_time" property="operateTime" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id, code,user_code,create_time,modify_time,operator,operate_time
    </sql>

    <select id="findAll" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM t_password
    </select>

    <insert id="insert" parameterType="org.moy.spring.test.example.domain.PasswordEntity">
        INSERT INTO t_password (
        <include refid="Base_Column_List"/>
        )
        VALUES (
        #{entity.id},
        #{entity.code},
        #{entity.userCode},
        #{entity.createTime},
        #{entity.modifyTime},
        #{entity.operator},
        #{entity.operateTime}
        )
    </insert>

    <delete id="delete">
        DELETE FROM t_password WHERE id= #{id}
    </delete>
</mapper>